{
  "compilerOptions": {
    "typeRoots": ["node_modules/@sapui5/ts-types", "node_modules/@types"],
    "target": "ES2017",
    "lib": ["ES2017", "DOM"],
    "noEmit": true,
    "skipLibCheck": true,
    "module": "none",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false
  },
  "include": ["webapp/**/*.js"],
  "paths": {
    "sap/ui/eventregistration/form/*": ["./webapp/*"]
  }
}
